while _G.keklolokay do
wait(20)
    if game.ReplicatedStorage.Remotes.Request:InvokeServer("Food") >= 99 then
for _, tool in ipairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
    if tool:IsA("Tool") and tool.Name == _G.train then
        tool.Parent = game:GetService("Players").LocalPlayer.Backpack
    end
end
wait(2)
local VirtualUser = game:GetService("VirtualUser")
    VirtualUser:CaptureController()
    VirtualUser:SetKeyDown(120)
    VirtualUser:SetKeyUp(120)
    end
end
